{
    "name": "@fjell/lib",
    "version": "4.3.1",
    "license": "UNLICENSED",
    "description": "Server-side Library for Fjell",
    "engines": {
        "node": ">=21"
    },
    "main": "dist/src/index.js",
    "exports": {
        ".": {
            "require": "./dist/src/index.js",
            "import": "./dist/src/index.js",
            "types": "./dist/src/index.d.ts"
        }
    },
    "type": "module",
    "scripts": {
        "build": "yarn run lint && tsc && tsc-alias -p tsconfig.json",
        "dev": "concurrently \"tsc -w\" \"tsc-alias -w\"",
        "lint": "eslint .",
        "clean": "rimraf dist",
        "test": "yarn run lint && yarn run jest --coverage"
    },
    "dependencies": {
        "@fjell/core": "^4.3.1",
        "@fjell/logging": "^4.3.4",
        "@google-cloud/firestore": "^7.11.0",
        "@google-cloud/storage": "^7.15.2",
        "dayjs": "^1.11.13",
        "deepmerge": "^4.3.1",
        "multer": "^1.4.5-lts.1",
        "sequelize": "^6.37.5",
        "specifier-resolution-node": "^1.1.4",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@tsconfig/recommended": "^1.0.8",
        "@types/jest": "^29.5.14",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.13.5",
        "@typescript-eslint/eslint-plugin": "^8.24.1",
        "@typescript-eslint/parser": "^8.24.1",
        "concurrently": "^9.1.2",
        "eslint": "^9.21.0",
        "jest": "^29.7.0",
        "nodemon": "^3.1.9",
        "rimraf": "^6.0.1",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.10",
        "typescript": "^5.7.3"
    },
    "packageManager": "yarn@4.6.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/getfjell/lib.git"
    }
}
